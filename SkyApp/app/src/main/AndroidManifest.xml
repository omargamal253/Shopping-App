<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skyapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AboutUsActivity"></activity>
        <activity android:name=".StartScreenActivity"></activity>
        <activity android:name=".MyOrdersActivity" />
        <activity android:name=".OrdersActivity" />
        <activity android:name=".FashionActivity" />
        <activity android:name=".ElectronicsActivity" />
        <activity android:name=".MobilesActivity" />
        <activity android:name=".LaptopActivity" />
        <activity android:name=".SuperMarketActivity" />
        <activity android:name=".DealsActivity" />
        <activity android:name=".CardActivity" />
        <activity android:name=".AddItemActivity" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>