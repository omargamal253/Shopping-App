<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skyapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.skyapp.CardActivity"></activity>
        <activity android:name="com.example.skyapp.AddItemActivity" />
        <activity android:name="com.example.skyapp.ProductActivity" />
        <activity android:name="com.example.skyapp.MainActivity" />
        <activity android:name="com.example.skyapp.LoginActivity" />
        <activity android:name="com.example.skyapp.RegisterActivity" />
        <activity android:name="com.example.skyapp.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>